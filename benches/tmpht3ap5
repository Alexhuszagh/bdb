#[macro_use]
extern crate bencher;
extern crate bdb;

use bencher::Bencher;
use bdb::db::uniprot::low_level::*;


fn accession_regex(bench: &mut Bencher) {
    bench.iter(|| {
        assert_eq!(AccessionRegex::validate().is_match("A2BC19")
        // A2BC19
    })
}

benchmark_group!(
    benches,
    accession_regex
);
benchmark_main!(benches);
