#[macro_use]
extern crate bencher;
extern crate bdb;

use bencher::Bencher;
use bdb::db::uniprot::low_level::*;


fn accession_regex(bench: &mut Bencher) {
    bench.iter(|| {
        AccessionRegex::validate().is_match("A2BC19") &&
        AccessionRegex::validate().is_match("P12345") &&
        AccessionRegex::validate().is_match("A0A022YWF9")
    })
}

fn mnemonic_regex(bench: &mut B)
fn gene_regex(bench: &mut B)
fn aminoacid_regex(bench: &mut B)
fn proteome_regex(bench: &mut B)
fn taxonomy_regex(bench: &mut B)
fn swissprot_header_regex(bench: &mut B)
fn trembl_header_regex(bench: &mut B)

benchmark_group!(
    benches,
    accession_regex
);
benchmark_main!(benches);
