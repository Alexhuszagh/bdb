#[macro_use]
extern crate bencher;
extern crate bdb;

use bencher::Bencher;
use bdb::db::uniprot::low_level::*;


fn accession_regex(bench: &mut Bencher) {
    bench.iter(|| {
        (0..1000).for_ea(0, |x, y| x + y)
    })
}

benchmark_group!(
    benches,
    accession_regex
);
benchmark_main!(benches);
